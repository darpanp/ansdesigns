
<!-- content -->
<div id="content"><div class="ic">More Website Templates at TemplateMonster.com!</div>
	<div class="container">
		<div class="wrapper">
			<div class="aside">
				&nbsp;
			</div>
			<div class="mainContent">
				<div class="wrapper">
					<!-- .box -->
					<div class="box">
						<h3>Admin Login</h3>
						<% unless flash[:error].blank? %>
						  <ul class="flash_error">
						  	<li><%= flash[:error] %></li>
						  </ul>
						  <br />
						<% end %>
						<%= form_tag support_adminlogin_path, :id=>'new_user' do %>
							<fieldset>
								<div class="field"><label for="text">Email</label><br /><%= text_field_tag 'username', nil, :class=>'text', :placeholder=>'registered email' %></div>
								<div class="field"><label for="text">Password</label><br /><%= password_field_tag 'password', nil, :class=>'password', :placeholder=>'registered password' %></div>
								<div class="wrapper">
									<%= submit_tag value = "Log In", :class => "submit fleft"%>
								</div>
							</fieldset>
						<% end %>
					</div>
					<br />
				<!-- /.box -->
				</div>	
			</div>
		</div>
	</div>
</div>